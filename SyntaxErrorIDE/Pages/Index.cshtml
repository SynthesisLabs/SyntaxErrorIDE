@page
@model SyntaxErrorIDE.Pages.IndexModel
@{
    ViewData["Title"] = "Home page";
}
<!doctype html>
<html lang="en">
<head>
    @await Html.PartialAsync("~/Pages/views/head.cshtml")
    <title>Home</title>
</head>
<body>
@await Html.PartialAsync("~/Pages/views/header.cshtml")
<main class="welcome">
    <div class="container">
        <h1>Code without limits</h1>
        <h1>SyntaxError IDE</h1>
        <a href="login">Start Coding</a>
    </div>
    @if (TempData["Message"] != null)
    { 
        @TempData["Message"]
    }
</main>
@await Html.PartialAsync("~/Pages/views/footer.cshtml")
</body>
</html>
